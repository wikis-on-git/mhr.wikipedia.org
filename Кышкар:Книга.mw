<includeonly><span class="citation" <!--
-->{{#if:{{{ref|}}}|<!-- есть ref
  -->id="{{anchorencode:CITEREF{{{ref}}}{{{ий|}}}{{{тиште|}}}}}"<!-- 
-->}}><!--
-->{{#if:{{{автор|}}}|<!-- есть автор
  --><i>{{{автор}}}</i><nowiki> </nowiki><!-- 
-->}}<!--
-->{{#if: {{{ужаш|}}}|<!-- есть часть
  -->{{#if: {{{негыз ужаш|}}}|<!-- есть ссылка на часть
    -->[{{{негыз ужаш}}} {{{ужаш}}}]| {{{ужаш}}}<!-- 
  -->}}<!--
  --><nowiki> // </nowiki><!-- добавляем разделить части
-->}}<!--
-->{{#if:{{{Викитеке|{{{викитеке|}}}}}}|<!-- есть викитека
  -->[[:s:{{{Викитеке|{{{викитеке}}}}}}|{{{лӱм|{{{вуймут}}}}}}]]|<!-- 
  -->{{#if:{{{негыз|}}}|<!-- нет викитеки но есть ссылка
    -->[{{{негыз}}} {{{лӱм|{{{вуймут}}}}}}]|<!-- 
    -->{{{лӱм|{{{вуймут}}}}}}<!-- нет ссылки добавляем название или заглавие
  -->}}<!--
-->}}<!-- 
-->{{#if:{{{оригинал|}}}|<!-- есть оригинал (точка не нужна)
  --><nowiki> = </nowiki>{{{оригинал}}} <!-- добавляем разделитель и оригинал
-->}}<!-- 
-->{{#if:{{{ответственный|}}}|<!-- есть ответственный, всегда добавляем точку
  --><nowiki> / </nowiki>{{{ответственный}}}.|<!-- иначе
  -->{{#if:{{{nodot|}}}||<!-- если добавление точки не запрещено, то
    -->.<!-- добавляем точку
  -->}}<!--
-->}}<!-- 
-->{{#if:{{{лӱм|{{{вуймут}}}}}}|<!-- если есть первое название или заглавие
  -->{{#if:{{{лӱм2|{{{вуймут2|}}}}}}|<!-- и есть второе название или заглавие, то перед ним уже есть точка
    -->&#32;{{{лӱм2|{{{вуймут2|}}}}}}<!-- добавляем второе название или заглавие
    -->{{#if:{{{оригинал2|}}}|<!-- есть оригинал2 (точка не нужна)
      --><nowiki> = </nowiki>{{{оригинал2}}} <!-- добавляем разделитель и оригинал
    -->}}<!-- 
    -->{{#if:{{{ответственный2|}}}|<!-- есть ответственный за второе название, всегда добавляем точку
      --><nowiki> / </nowiki>{{{ответственный2}}}.|<!-- иначе
      -->{{#if:{{{nodot2|}}}||<!-- если добавление точки для второго названия не запрещено, то
        -->.<!-- добавляем точку
      -->}}<!--
    -->}}<!-- 
  -->}}<!-- 
-->}}<!-- 
-->{{#if:{{{савыш|}}}|<!-- есть издание
  -->&nbsp;— {{{савыш}}}.<!--  добавляем разделитель, издание и точку после
-->}}<!--
-->{{#switch:{{#if:{{{ола|{{{вер|}}}}}}|в}}{{#if:{{{савыктыш|}}}|с}}{{#if:{{{ий|}}}|ий}}
  |миг=&nbsp;— {{указание места в библиоссылке|{{{ола|{{{вер}}}}}}}}: {{{савыктыш}}}, {{{ий}}}.
  |ми=&nbsp;— {{указание места в библиоссылке|{{{ола|{{{вер}}}}}}}}: {{{савыктыш}}}.
  |мг=&nbsp;— {{указание места в библиоссылке|{{{ола|{{{вер}}}}}}}}, {{{ий}}}.
  |иг=&nbsp;— {{{савыктыш}}}, {{{ий}}}.
  |м=&nbsp;— {{указание места в библиоссылке|{{{ола|{{{вер}}}}}}|.}}
  |и=&nbsp;— {{{савыктыш}}}.
  |г=&nbsp;— {{{ий}}}.
}}<!-- далее всегда добавляем точку в конце
-->{{#if:{{{том как есть|}}}|&nbsp;—&#32;{{{том как есть}}}.}}<!--
-->{{#if:{{{том|}}}|&nbsp;— Т.&nbsp;{{{том}}}.}}<!--
-->{{#if:{{{volume|}}}|&nbsp;— Vol.&nbsp;{{{volume}}}.}}<!--
-->{{#if:{{{band|}}}|&nbsp;— B.&nbsp;{{{band}}}.}}<!--
-->{{#if:{{{лаштык-влак как есть|}}}|&nbsp;—&#32;{{{лаштык-влак как есть}}}.}}<!--
-->{{#if:{{{лаштык-влак|}}}|&nbsp;— Л.&nbsp;<!--
-->{{#if:{{{столбик-влак|}}}|&#91;{{{лаштык-влак}}}&#93; (стб.&nbsp;{{{столбик-влак}}}).|{{{лаштык-влак}}}.}}}}<!--
-->{{#if:{{{страниц как есть|}}}|&nbsp;—&#32;{{{страниц как есть}}}.}}<!--
-->{{#if:{{{лаштык|}}}|&nbsp;— {{{лаштык}}}&nbsp;л.}}<!--
-->{{#if:{{{pages|}}}|&nbsp;— P.&nbsp;<!--
-->{{#if:{{{columns|}}}|&#91;{{{pages}}}&#93; (col.&nbsp;{{{columns}}}).|{{{pages}}}.}}}}<!--
-->{{#if:{{{seite|}}}|&nbsp;— S.&nbsp;<!--
-->{{#if:{{{kolonnen|}}}|&#91;{{{seite}}}&#93; (Kol.&nbsp;{{{kolonnen}}}).|{{{seite}}}.}}}}<!--
-->{{#if:{{{allpages|}}}|&nbsp;— {{{allpages|}}}&nbsp;p.}}<!--
-->{{#if:{{{alleseiten|}}}|&nbsp;— {{{alleseiten|}}}&nbsp;s.}}<!--
-->{{#if:{{{серий|}}}|&nbsp;— ({{{серий}}}).}}<!--
-->{{#if:{{{тираж|}}}|&nbsp;— {{nobr|{{formatnum:{{{тираж}}}}} экз.}}}}<!--
-->{{#if:{{{isbn|}}}|&nbsp;— ISBN {{{isbn}}}}}</span></includeonly><noinclude>
{{doc}}
</noinclude>
